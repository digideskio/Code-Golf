<!DOCTYPE html>
<html>
<head>
  <title>Golf</title>
  <%= stylesheet_link_tag    'application' %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  
  <div id="container">
    <%= render 'layouts/header'%>
    <br>
    <%= render 'layouts/sidebar' %>
    
    <div id="content">

      <section id="flash">
        <% flash.each do |key, value| %>
          <div class="flash <%= key %>"><%= value %></div>
        <% end %>
      </section>

      <%= yield %>
    </div>
    


  </div>


</body>
</html>
